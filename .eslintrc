{
    "rules": {
      "fp/no-arguments": "error",
      "fp/no-class": "error",
      "fp/no-delete": "error",
      "fp/no-events": "error",
      "fp/no-get-set": "error",
      "fp/no-let": "error",
      "fp/no-loops": "error",
      "fp/no-mutating-assign": "error",
      "fp/no-mutating-methods": "error",
      "fp/no-mutation": "error",
      "fp/no-proxy": "error",
      "fp/no-rest-parameters": "error",
      "fp/no-throw": "error",
      "fp/no-valueof-field": "error",
      "no-var": "error"
    },
    "env": {
       "es6": true
    },
    "plugins": [
      "fp"
    ],
    "parser": "babel-eslint"
}